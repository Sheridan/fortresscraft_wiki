{
	"name": "FCE Extension",
	"author": "Sheridan",
	"version": "1.0.0",
	"url": "https://fortresscraft.info",
	"descriptionmsg": "FortressCraft data related plugin",
	"license-name": "GPL-2.0+",
	"type": "parserhook",
	"AutoloadClasses": { "fce": "fce.class.php"	},
	"ExtensionMessagesFiles": { "fceMagic": "fce.i18n.php" },
	"Hooks": {
		"ParserFirstCallInit": [ "fce::onParserFirstCallInit"],
		"BeforePageDisplay": "fce::onBeforePageDisplay"
