	"ResourceModules": { "ext.fce": { "styles": "styles/ext.fce.css", "scripts": "scripts/fce.page.js" } },
	"ResourceFileModulePaths": { "localBasePath": "", "remoteExtPath": "fce" },
	"manifest_version": 1
}
